void calc(){  //计算器的实现
    char i;
    char flag;
	int numWid=8;//数字宽度
	char st=4;//算式起始行
	char buf[10];
    int s1,s2,num1,num2;  // s1、s2表示操作数的位数，num1、num2表示两个操作数
	DispBmp(calcMenu);
    for(;;){
	   num1=0;
	   num2=0;
	   InitLCD();
	  DispBmp(calcMenu);
       for(s1=0;s1<7;s1++){
          do{
              i=GetKeyValue();
          }while(i==-1);
		  WaitKeyOff();
		 // if((s1==6)&&())
          if(i>=0&&i<=9&&s1!=6){
		     WriteSZ(s1*numWid,st,i);
			 num1 = num1*10+i;
		  }
          else if((i==0x0a || i==0x0b || i==0x0c || i==0x0d)&&(s1!=0)){  //加减乘除的选择按键
		     flag=i;
		     switch(flag){
               case 0x0a: LCD_WriteHZ(s1*numWid,st,fu);break;  //加
			   case 0x0b: LCD_WriteHZ(s1*numWid,st,fu+32);break;  //减
			   case 0x0c: LCD_WriteHZ(s1*numWid,st,fu+32*2);break;  //乘
			   case 0x0d: LCD_WriteHZ(s1*numWid,st,fu+32*3);break;   //除
			 }
			 break;
          }
		  else if(i==0x0f)
		  	goto exitt;
		  else if((i>=0&&i<=9)&&s1==6){
		  	flag=0x0a;
			LCD_WriteHZ(s1*numWid,st,fu);
		  }
		  else if((i==0x0a || i==0x0b || i==0x0c || i==0x0d|| i==0x0e)&&(s1==0))//一开始输入符号
		  	goto exitt;//
       }
       for(s2=0;s2<7;s2++){
          do{
              i=GetKeyValue();
          }while(i==-1);
		  WaitKeyOff();
          if(i>=0&&i<=9&&s1!=6){
		     s1++;
		     WriteSZ(s1*numWid+8,st,i);
			 num2 = num2*10+i;
		  }
          else if(i==0x0e&&(s1!=0)){
		     s1++;
		     LCD_WriteHZ(0,st+2,fu+32*4);
             break;
          }
		  else if(i==0x0f)
		  	goto exitt;
		  else if((i>=0&&i<=9)&&s1==6){
		  //	flag=0x0a;
			LCD_WriteHZ(0,st+2,fu+32*4);
		  }
		  else if(i==0x0e&&(s1==0)){
		     LCD_WriteHZ(s1*numWid,st,Kong);
		     LCD_WriteHZ(0,st+2,fu+32*4);
            goto nextt;
          }
       }
       switch(flag){
          case 0x0a: num1=num1+num2;break;
		  case 0x0b: num1=num1-num2;break;
		  case 0x0c: num1=num1*num2;break;
		  case 0x0d: num1=num1/num2;break;
       }
nextt:	   for(s1=0;num1!=0;s1++){
	      buf[s1]=num1%10;
		  num1=num1/10;
	   }
	   for(s2=0;s2<s1;s2++){
	      WriteSZ(s2*numWid+16,st+2,buf[s1-1-s2]);
	   }
	   do{
           i=GetKeyValue();
          }while(i==-1);
		WaitKeyOff();
		s2++;
		if(i==0x0f){
		
		break;}
    }}


