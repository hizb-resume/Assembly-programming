char code Dot[]={
0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x00,0x30,0x30,0x00,0x00,0x00,0x00,0x00,
};
#define SELECTCS 0
void LCD_WaitReady1(void) //判断LCD控制芯片KS0108是否忙
{
	LCD_DATA=0xff;
	_nop_();
	LCD_CS2=SELECTCS;
	LCD_RW=1;
	LCD_RS=0;
	LCD_E=1;
//	while (LCD_DATA&0x80);
	LCD_CS2=!SELECTCS;
}
void LCD_WaitReady2(void) //判断LCD控制芯片KS0108是否忙
{
	LCD_DATA=0xff;
	_nop_();
	LCD_CS1=SELECTCS;
	LCD_RW=1;
	LCD_RS=0;
	LCD_E=1;
	while (LCD_DATA&0x80);
	LCD_CS1=!SELECTCS;
}
void LCD_WriteCommand1(char ch) //向LCD控制芯片KS0108发送控制命令
{
	LCD_WaitReady1();
	LCD_CS2=SELECTCS;
	LCD_RW=0;
	LCD_RS=0;
	LCD_DATA=ch;
	LCD_E=1;
	LCD_E=0;
	LCD_CS2=!SELECTCS;
}
void LCD_WriteCommand2(char ch)	//向LCD控制芯片KS0108发送控制命令
{
	LCD_WaitReady2();
	LCD_CS1=SELECTCS;
	LCD_RW=0;
	LCD_RS=0;
	LCD_DATA=ch;
	LCD_E=1;
	LCD_E=0;
	LCD_CS1=!SELECTCS;
}
void LCD_WriteData1(char ch)	//向LCD控制芯片KS0108发送数据命令
{
	LCD_WaitReady1();
	LCD_CS2=SELECTCS;
	LCD_RW=0;
	LCD_RS=1;
	LCD_DATA=ch;
	LCD_E=1;
	LCD_E=0;
	LCD_CS2=!SELECTCS;
}
void LCD_WriteData2(char ch)	//向LCD控制芯片KS0108发送数据命令
{
	LCD_WaitReady2();
	LCD_CS1=SELECTCS;
	LCD_RW=0;
	LCD_RS=1;
	LCD_DATA=ch;
	LCD_E=1;
	LCD_E=0;
	LCD_CS1=!SELECTCS;
}



void LCD_WriteHZ(char x,char y,char *Dot) //显示16*16点阵汉字
{
char i;
	for (i=0;i<32;i++)
	{
		if ((x+i%16)<64)
		{
			LCD_WriteCommand1(0xB8+y+i/16);
			LCD_WriteCommand1(0x40+x+i%16);
			LCD_WriteData1(Dot[i]);
		}
		else if ((x+i%16)<128)
		{
			LCD_WriteCommand2(0xB8+y+i/16);
			LCD_WriteCommand2(0x40+x-64+i%16);
			LCD_WriteData2(Dot[i]);
		}
		else 
		{
			LCD_WriteCommand1(0xB8+y+i/16);
			LCD_WriteCommand1(0x40+x-128+i%16);
			LCD_WriteData1(Dot[i]);
		}
	}
}

void LCD_WriteSZ(char x,char y,char *Dot) //显示16*8点阵数字
{
char i;
	for (i=0;i<16;i++)
	{
		if ((x+i%8)<64)
		{
			LCD_WriteCommand1(0xB8+y+i/8);
			LCD_WriteCommand1(0x40+x+i%8);
			LCD_WriteData1(Dot[i]);
		}
		else if ((x+i%8)<128)
		{
			LCD_WriteCommand2(0xB8+y+i/8);
			LCD_WriteCommand2(0x40+x-64+i%8);
			LCD_WriteData2(Dot[i]);
		}
		else
	    {
			LCD_WriteCommand1(0xB8+y+i/8);
			LCD_WriteCommand1(0x40+x-128+i%8);
			LCD_WriteData1(Dot[i]);
		}
	}
}
void WriteSZ(char x, char y, char z)
{
     switch (z)
    {  
        case 0:
		      LCD_WriteSZ(x,y,Zero);
			  break;
	case 1:
			  LCD_WriteSZ(x,y,One);
			  break;
	case 2:
			   LCD_WriteSZ(x,y,Two);
			    break;
	case 3:
		      LCD_WriteSZ(x,y,Three);
			  break;
    case 4:
		      LCD_WriteSZ(x,y,Four);
			  break;
	case 5:
		      LCD_WriteSZ(x,y,Five);
			  break;
	case 6:
		      LCD_WriteSZ(x,y,Six);
			  break;
	case 7:
	        LCD_WriteSZ(x,y,Seven);
			  break;
	case 8:
		      LCD_WriteSZ(x,y,Eight);
			  break;
	case 9:
		      LCD_WriteSZ(x,y,Nine);			  
   }
}

void WriteSZ2(char x, char y, char z)
{
     switch (z)
    {         
	case 1:
			  LCD_WriteSZ(x,y,One);
			  break;
	case 2:
			   LCD_WriteSZ(x,y,Two);
			    break;
	case 3:
		      LCD_WriteSZ(x,y,Three);
			  break;
    case 4:
		      LCD_WriteSZ(x,y,Four);
			  break;
	case 5:
		      LCD_WriteSZ(x,y,Five);
			  break;
	case 6:
		      LCD_WriteSZ(x,y,Six);
			  break;
	case 7:
	        LCD_WriteSZ(x,y,Seven);
			  break;
	case 8:
		      LCD_WriteSZ(x,y,Eight);
			  break;
	case 9:
		      LCD_WriteSZ(x,y,Nine);			  
   }
}

void InitLCD(void)	        //初始化LCD
{
	char i,j;
	LCD_RST=0;
	Delay1ms(1);
	LCD_RST=1;
    LCD_WriteCommand1(0xc0);	
	LCD_WriteCommand2(0xc0);
	LCD_WriteCommand1(0x3f);  	//开显示
	LCD_WriteCommand2(0x3f);
for (j=0;j<8;j++)			//清屏
	{
		LCD_WriteCommand1(0xB8+j);	//清左半屏
		LCD_WriteCommand1(0x40);
		for (i=0;i<64;i++)
		LCD_WriteData1(0x00);
        LCD_WriteCommand2(0xB8+j);	//清右半屏
		LCD_WriteCommand2(0x40);
		for (i=0;i<64;i++)
		LCD_WriteData2(0x00);
	}
}
void DispBmp(char *buf)	//显示一幅位图
{
int i,j;
	for (j=0;j<8;j++)			//显示位图
	{
		LCD_WriteCommand1(0xB8+j);	
		LCD_WriteCommand1(0x40);
		for (i=0;i<64;i++)
			LCD_WriteData1(buf[(j*2)*64+i]);
		LCD_WriteCommand2(0xB8+j);	
		LCD_WriteCommand2(0x40);
		for (i=0;i<64;i++)
			LCD_WriteData2(buf[(j*2+1)*64+i]);
	}
}
void Left(char *buf)//图片左移
{
   int i, j, k;
   for (k = 0; k < 128; ++k)
   {
       for (j = 0; j < 8; ++j)
	   {
           	LCD_WriteCommand1(0xB8+j);	
	    	LCD_WriteCommand1(0x40);
	    	for (i=0;i<64;i++)
		 	    LCD_WriteData1(buf[(j*2)*64+(i+k)%128]);
            LCD_WriteCommand2(0xB8+j);	
	    	LCD_WriteCommand2(0x40);
	    	for (i=0;i<64;i++)
		 	    LCD_WriteData2(buf[(j*2)*64+(i+k+64)%128]);
       }
	    Delay1ms(300);
   }
}

void Right(char *buf)//图片右移
{
   int i, j, k;
   for (k = 0; k < 128; ++k)
   {
       for (j = 0; j < 8; ++j)
	   {
           	LCD_WriteCommand1(0xB8+j);	
	    	LCD_WriteCommand1(0x40);
	    	for (i=0;i<64;i++)
		 	    LCD_WriteData1(buf[(j*2)*64+(i+128-k)%128]);
            LCD_WriteCommand2(0xB8+j);	
	    	LCD_WriteCommand2(0x40);
	    	for (i=0;i<64;i++)
		 	    LCD_WriteData2(buf[(j*2)*64+(i+128+64-k)%128]);
       }
	    Delay1ms(300);
   }
}

void Delay1s(unsigned char T);
void Up(char *buf)//图片上移
{
   int i, j, k;
   for (k = 0; k < 9; ++k)
   {     
       for (j = 0; j < 8; ++j)
	   {
           	LCD_WriteCommand1(0xB8+j);	
	    	LCD_WriteCommand1(0x40);
	    	for (i=0;i<64;i++)
		 	    LCD_WriteData1(buf[((j*2)*64+(i+k*128))%1024]);
            LCD_WriteCommand2(0xB8+j);	
	    	LCD_WriteCommand2(0x40);
	    	for (i=0;i<64;i++)
		 	    LCD_WriteData2(buf[((j*2+1)*64+(i+k*128))%1024]);
       }
	   Delay1ms(500);
   }
}

void Down(char *buf)//图片下移
{
   int i, j, k;
   for (k = 0; k < 9; ++k)
   {   
       for (j = 0; j < 8; ++j)
	   {
           	LCD_WriteCommand1(0xB8+j);	
	    	LCD_WriteCommand1(0x40);
	    	for (i=0;i<64;i++)
		 	    LCD_WriteData1(buf[((j*2)*64+i+1024-k*128)%1024]);
            LCD_WriteCommand2(0xB8+j);	
	    	LCD_WriteCommand2(0x40);
	    	for (i=0;i<64;i++)
		 	    LCD_WriteData2(buf[((j*2+1)*64+i+ 1024-k*128)%1024]);
       }
	   Delay1ms(500);
   }
}

void DispBmp1(char *buf,int n,int m)	//显示一幅位图//128*16,n为页值，m为列值
{
int i,j;
for (j=0;j<2;j++)			//显示位图
	{
	if(m<=63)
	{
	    LCD_WriteCommand1(0xB8+j+n);	
		LCD_WriteCommand1(0x40);
		for(i=128-m;i<=127;i++)
		    LCD_WriteData1(buf[i+128*j]);
		for(i=0;i<=63-m;i++)
		    LCD_WriteData1(buf[i+128*j]);
		LCD_WriteCommand2(0xB8+j+n);	
		LCD_WriteCommand2(0x40);
		for(i=64-m;i<=127-m;i++)	
			LCD_WriteData2(buf[128*j+i]);
		}
	else
		{ 
	    LCD_WriteCommand2(0xB8+j+n);	
		LCD_WriteCommand2(0x40);
		for(i=192-m;i<=127;i++)
			LCD_WriteData2(buf[128*j+i]);
		for(i=0;i<=127-m;i++)
	    	LCD_WriteData2(buf[128*j+i]);
		LCD_WriteCommand1(0xB8+j+n);	
		LCD_WriteCommand1(0x40);
		for(i=128-m;i<=191-m;i++)
			LCD_WriteData1(buf[i+128*j]);
		}
}
}
void Display (char *DispBuf,char ShowDot,char x,char y)  //液晶屏显示温度、电压，压力值
{
	char i,ch,j=-16;
	ShowDot--;
	for (i=2;i<6;i++)
	{
		j+=16;
		ch=DispBuf[i];
		switch(ch)
		{	case 0:  LCD_WriteSZ(x+j,y,led0);   break;
            case 1:  LCD_WriteSZ(x+j,y,led1);  break;
			case 2:  LCD_WriteSZ(x+j,y,led2);  break;
			case 3:  LCD_WriteSZ(x+j,y,led3);  break;
			case 4:  LCD_WriteSZ(x+j,y,led4);  break;
			case 5:  LCD_WriteSZ(x+j,y,led5);  break;
			case 6:  LCD_WriteSZ(x+j,y,led6);  break;
			case 7:  LCD_WriteSZ(x+j,y,led7);  break;
			case 8:  LCD_WriteSZ(x+j,y,led8);  break;
			case 9:  LCD_WriteSZ(x+j,y,led9);  break;
			default:  LCD_WriteSZ(x+j,y,led);  break;
			}
		if(i==ShowDot)
		{
			LCD_WriteSZ(x+j+8,y,Dot);
		}
	}
}
unsigned int GetADCValue(char No);
char GetKeyValue(void);
void WaitKeyOff(void);
void menulcd ()            //显示菜单
{
	InitLCD();
	DispBmp(screen4);
}
void TestLCD()
{
    char i;
	InitLCD();              //显示江苏大学计算机学院
	LCD_WriteHZ(31,2,Jiang); 
	LCD_WriteHZ(47,2,Su); 
    LCD_WriteHZ(64,2,Da); 
	LCD_WriteHZ(80,2,Xue); 
	LCD_WriteHZ(24,4,Ji); 
	LCD_WriteHZ(40,4,Suan); 
	LCD_WriteHZ(56,4,Jii); 
	LCD_WriteHZ(72,4,Xue); 
	LCD_WriteHZ(88,4,Yuan); 
    Delay1s(3);
	
	for (i=0;i<64;i++)
	{
		Delay1ms(50);
		LCD_WriteCommand1(0xc0+i%64);	//设置起始行,实现向上滚动
		LCD_WriteCommand2(0xc0+i%64);
	}
	Delay1s(3);
  
	for (i = 0; i < 2; ++i)
	{
	InitLCD();	           //显示2016年单片机课程设计
	LCD_WriteSZ(40,2,Two);
	LCD_WriteSZ(48,2,Zero);
	LCD_WriteSZ(56,2,One);
	LCD_WriteSZ(64,2,Six);
	LCD_WriteHZ(72,2,Nian);
	LCD_WriteHZ(8,4,Dan);
	LCD_WriteHZ(24,4,Pian);
	LCD_WriteHZ(40,4,Jii);
	LCD_WriteHZ(56,4,Ke);
	LCD_WriteHZ(72,4,Cheng);
	LCD_WriteHZ(88,4,She);
	LCD_WriteHZ(104,4,Ji);
    Delay1s(1);
	InitLCD();	           //显示液晶点阵显示屏程序设计
	LCD_WriteHZ(32,2,Ye);
	LCD_WriteHZ(48,2,Jing);
	LCD_WriteHZ(64,2,Dian);
	LCD_WriteHZ(80,2,Zhen);
	LCD_WriteHZ(8,4,Xian);
	LCD_WriteHZ(24,4,Shi);
	LCD_WriteHZ(40,4,Ping);
	LCD_WriteHZ(56,4,Cheng);
	LCD_WriteHZ(72,4,Xu);
	LCD_WriteHZ(88,4,She);
	LCD_WriteHZ(104,4,Ji);
	Delay1s(1);
	}

		for (i = 0; i < 127;i++)
		{
			InitLCD();                             //显示姓名班级学号
			LCD_WriteHZ((4+128-i)%128,1,Xing);
			LCD_WriteHZ((20+128-i)%128,1,Ming);
        	LCD_WriteSZ((36+128-i)%128,1,Maohao);
	        LCD_WriteHZ((44+128-i)%128,1,Zeng);
        	//LCD_WriteHZ((60+128-i)%128,1,Hai);
        	LCD_WriteHZ((76+128-i)%128,1,Biao);
			LCD_WriteHZ((4+128-i)%128,3,Ban);
			LCD_WriteHZ((20+128-i)%128,3,Jiii);
			LCD_WriteSZ((36+128-i)%128,3,Maohao);
			LCD_WriteHZ((44+128-i)%128,3,Ji);
			LCD_WriteHZ((60+128-i)%128,3,Suan);
			LCD_WriteHZ((76+128-i)%128,3,Jii);
			LCD_WriteSZ((92+128-i)%128,3,One); 
			LCD_WriteSZ((100+128-i)%128,3,Three);
			LCD_WriteSZ((108+128-i)%128,3,Zero); 
			LCD_WriteSZ((116+128-i)%128,3,Two);
			LCD_WriteHZ((4+128-i)%128,5,Xue);
			LCD_WriteHZ((20+128-i)%128,5,Hao);
			LCD_WriteSZ((36+128-i)%128,5,Maohao);
			LCD_WriteSZ((44+128-i)%128,5,Three);
			LCD_WriteSZ((52+128-i)%128,5,One);
			LCD_WriteSZ((60+128-i)%128,5,Three);
			LCD_WriteSZ((68+128-i)%128,5,Zero);
			LCD_WriteSZ((76+128-i)%128,5,Six);
			LCD_WriteSZ((84+128-i)%128,5,Zero);
			LCD_WriteSZ((92+128-i)%128,5,Two);
			LCD_WriteSZ((100+128-i)%128,5,Zero);
			LCD_WriteSZ((108+128-i)%128,5,Five);
			LCD_WriteSZ((116+128-i)%128,5,Zero);
			Delay1ms(300);
			
		}
}
void zuoyouyi()//图片滚动
{ 
    InitLCD();
    DispBmp(screen3);
    Delay1s(1);
    Left(screen3);
    Delay1s(2);
    Right(screen3);
    Delay1s(2);
    Up(screen3);
    Delay1s(2);
    Down(screen3);
    Delay1s(2);
}

void caiji ()       //温度，电压，压力采集
{
	char i;
int st=8;
	unsigned int w;
	char No,buf[6];
	No=1;      
	w = GetADCValue(1);
	for (;;)	{
		i=GetKeyValue();
		if ((i>=0)&&(i<=3))//如果按键大于3，则不读入键值
			No=i;
		Delay1ms(250);
		buf[0]= No;
		buf[1]= ' ';
		buf[2]=(w%10000)/1000;
		buf[3]=(w%1000)/100;
		buf[4]=(w%100)/10;
		buf[5]=(w%10);
		if (No==1)
		{
			InitLCD();
			DispBmp(tem);//按1采集到温度
			Display(buf,4,st+30,4);
			LCD_WriteHZ(st+102,4,du);
		}
		else if (No==2)
		{
			InitLCD();
			DispBmp(volte);//按2采集到电压
			Display(buf,3,st+30,4);
				LCD_WriteSZ(st+102,4,dianya);
		}			  	
		else if (No==3)
		{	
			InitLCD();
			DispBmp(pres);//按3采集到压力
			Display(buf,4,st+30,4);
				LCD_WriteSZ(st+102,4,yali);
		}
		else
		   break;//按0回到主菜单			  	
		w = GetADCValue(No);
	}
}

char nian1,nian2,nian3,nian4,yue1,yue2,ri1,ri2;
char shih,shil,fenh,fenl,miaoh,miaol,haoh,haol;

//void showMiaobiao(char shih,char shil,char fenh,char fenl,char miaoh,char miaol,char haoh,char haol)
void showMiaobiao(int cas)
{
	char mm=3;//日期起始行
	char m=mm+2;//秒表数字的起始行
	char hs=32;//秒表时分秒的起始列
	char hss=8;//秒表年月日的起始列
//	InitLCD();              //
	if(cas==2)
		DispBmp(miaobiaoModify);//
	else
		DispBmp(miaobiaoCaidan);


	WriteSZ(hss,mm,nian1); 
	WriteSZ(hss+8,mm,nian2); 
	WriteSZ(hss+16,mm,nian3); 
	WriteSZ(hss+24,mm,nian4); 
	LCD_WriteHZ(hss+32,mm,Nian);
	WriteSZ(hss+48,mm,yue1); 
	WriteSZ(hss+56,mm,yue2); 
	LCD_WriteHZ(hss+64,mm,Yue);
	WriteSZ(hss+80,mm,ri1); 
	WriteSZ(hss+88,mm,ri2); 
	LCD_WriteHZ(hss+96,mm,Ri);


	WriteSZ(hs,m,shih); 
	WriteSZ(hs+8,m,shil); 
	LCD_WriteSZ(hs+16,m,Maohao); 
	WriteSZ(hs+24,m,fenh); 
	WriteSZ(hs+32,m,fenl); 
	LCD_WriteSZ(hs+40,m,Maohao); 
	WriteSZ(hs+48,m,miaoh); 
	WriteSZ(hs+56,m,miaol);
//	LCD_WriteSZ(hs+64,m,Maohao);  
//	WriteSZ(hs+72,m,haoh); 
//	WriteSZ(hs+80,m,haol); 

}

void refresh(){

if(nian1>9)
	nian1=9;
	if(nian2>9)
	nian2=9;
	if(nian3>9)
	nian3=9;
	if(nian4>9)
	nian4=9;

if(yue1>1)
	yue1=1;
if(yue1==0&&yue2>9)
yue2=9;
if(yue1==0&&yue2==0)
yue2=1;
if(yue1==1&&yue2>2)
yue2=2;
if ((((yue1 == 0) && ((yue2 == 1) || (yue2 == 3) || (yue2 == 5) || (yue2 == 7) || (yue2 == 8))) || ((yue1 == 1) && ((yue2 == 0) || (yue2 == 2))))){
if(ri1>3)
ri1=3;
if(ri2>9)
ri2=0;
if(ri1==0&&ri2==0)
ri2=1;
if(ri1==3&&ri2>1)
ri2=1;
}
if ((((yue1 == 0) && ((yue2 == 4) || (yue2 == 6) || (yue2 == 9) )) || ((yue1 == 1) && ((yue2 == 1) )))){//30天
if(ri1>3)
ri1=3;
if(ri2>9)
ri2=0;
if(ri1==0&&ri2==0)
ri2=1;
if(ri1==3&&ri2>0)
ri2=0;
}
if (((yue1==0)&&(yue2==2))){//28天
if(ri1>2)
ri1=2;
if(ri2>9)
ri2=0;
if(ri1==0&&ri2==0)
ri2=1;
if(ri1==2&&ri2>8)
ri2=8;
}

if(shih>2)
shih=2;
if(shih==2&&shil>3)
shil=3;
if(shil>9)
shil=0;
if(fenh>5)
fenh=0;
if(fenl>9)
fenl=0;
if(miaoh>5)
miaoh=0;
if(miaol>9)
miaol=0;

}

void modifyTime(){

	char mm=3;//日期起始行
	char m=mm+2;//秒表数字的起始行
	char hs=32;//秒表时分秒的起始列
	char hss=8;//秒表年月日的起始列
	int j=0;
	char i;
	showMiaobiao(2);
//	LCD_WriteSZ(hss,mm,Line); 
	for (;;)	{
		i=GetKeyValue();

if(i==-1){
		if(j==0){
		LCD_WriteSZ(hss+0,mm,Line); }
		else if(j==1){
		LCD_WriteSZ(hss+8,mm,Line); }
		else if(j==2){
		LCD_WriteSZ(hss+16,mm,Line); }
		else if(j==3){
		LCD_WriteSZ(hss+24,mm,Line); }
		else if(j==4){
		LCD_WriteSZ(hss+48,mm,Line); }
		else if(j==5){
		LCD_WriteSZ(hss+56,mm,Line); }
		else if(j==6){
		LCD_WriteSZ(hss+80,mm,Line); }

		else if(j==7){
		LCD_WriteSZ(hss+88,mm,Line);}
		else if(j==8){
		LCD_WriteSZ(hs,m,Line); }
		else if(j==9){
		LCD_WriteSZ(hs+8,m,Line); }
		else if(j==10){
		LCD_WriteSZ(hs+24,m,Line); }
		else if(j==11){
		LCD_WriteSZ(hs+32,m,Line); }
		else if(j==12){
		LCD_WriteSZ(hs+48,m,Line); }
		else if(j==13){
		LCD_WriteSZ(hs+56,m,Line); }

		Delay1ms(300);
		showMiaobiao(2);
}


		if(i!=-1)
          {
		   // Send7279Byte(0xC8);	//发送键码值，按方式1译码下载显示 
		 //   Send7279Byte(KeyValue%16);
		  //   NOSELECT7279;     	//置CS高电平 
		    WaitKeyOff();
           

	//	if ((i>=0)&&(i<=9))//

//	shih,shil,fenh,fenl,miaoh,miaol,haoh,haol;



		if(j==0){
		nian1=i;
	//	WriteSZ(hss,mm,i); 
		j++;
		LCD_WriteSZ(hss+8,mm,Line); }

		else if(j==1){
		nian2=i;
	//	WriteSZ(hss+8,mm,i); 
		j++;
		LCD_WriteSZ(hss+16,mm,Line); }

		else if(j==2){
		nian3=i;
	//	WriteSZ(hss+16,mm,i); 
		j++;
		LCD_WriteSZ(hss+24,mm,Line); }

		else if(j==3){
		nian4=i;
	//	WriteSZ(hss+24,mm,i); 
		j++;
		LCD_WriteSZ(hss+48,mm,Line); }


		else if(j==4){
		yue1=i;
	//	WriteSZ(hss+48,mm,i); 
		j++;
		LCD_WriteSZ(hss+56,mm,Line); }

		else if(j==5){
		yue2=i;
	//	WriteSZ(hss+56,mm,i); 
		j++;
		LCD_WriteSZ(hss+80,mm,Line); }

		else if(j==6){
		ri1=i;
	//	WriteSZ(hss+80,mm,i); 
		j++;
		LCD_WriteSZ(hss+88,mm,Line); }

		else if(j==7){
		ri2=i;
	//	WriteSZ(hss+88,mm,i); 
		j++;
		LCD_WriteSZ(hs,m,Line); }



		else if(j==8){
		shih=i;
	//	WriteSZ(hs,m,i); 
		j++;
		LCD_WriteSZ(hs+8,m,Line); }


		else if(j==9){
		shil=i;
	//	WriteSZ(hs+8,m,i); 
		j++;
		LCD_WriteSZ(hs+24,m,Line); }

		else if(j==10){
		fenh=i;
	//	WriteSZ(hs+24,m,i); 
		j++;
		LCD_WriteSZ(hs+32,m,Line); }

		else if(j==11){
		fenl=i;
	//	WriteSZ(hs+32,m,i); 
		j++;
		LCD_WriteSZ(hs+48,m,Line); }

		else if(j==12){
		miaoh=i;
	//	WriteSZ(hs+48,m,i); 
		j++;
		LCD_WriteSZ(hs+56,m,Line); }

		else if(j==13){
		miaol=i;
		refresh();
		showMiaobiao(1);
	//	WriteSZ(hs+56,m,i); 
		break;}

		refresh();
		showMiaobiao(2);

}			

			}

}

void miaobiao()//秒表
 	{


	char i;
	int pause=0;
	int No;
	
	shih=0;	shil=0;fenh=0;fenl=0;miaoh=0;miaol=0;haoh=0;haol=0;
	nian1=2,nian2=0,nian3=1,nian4=6,yue1=0,yue2=7,ri1=0,ri2=1;
    
//	unsigned int w;
//	char No,buf[6];
	No=7;      
//	w = GetADCValue(1);

//showMiaobiao(shih,shil,fenh,fenl,miaoh,miaol,haoh,haol);
showMiaobiao(1);
	for (;;)	{
		i=GetKeyValue();
		if(i!=-1)
		    WaitKeyOff();
		if ((i>=0)&&(i<=4))//如果按键大于2，则不读入键值
			No=i;
		else
			No=7;
	//	Delay1ms(250);
	/*	buf[0]= No;
		buf[1]= ' ';
		buf[2]=(w%10000)/1000;
		buf[3]=(w%1000)/100;
		buf[4]=(w%100)/10;
		buf[5]=(w%10);*/
		if (No==1)//开始
		{

		//	InitLCD();

		//	if(pause==0)
				pause=1;
		//	else
		//		pause=0;

		//	DispBmp(tem);//按1采集到温度
		//	Display(buf,4,30,4);
		//	LCD_WriteHZ(102,4,du);
		}
		else if (No==3)//清0
		{

		//	InitLCD();

			shih=0;	shil=0;fenh=0;fenl=0;miaoh=0;miaol=0;haoh=0;haol=0;
			nian1=2,nian2=0,nian3=1,nian4=6,yue1=0,yue2=7,ri1=0,ri2=1;
			showMiaobiao(1);
			pause=0;


		//	DispBmp(volte);//按2采集到电压
		//	Display(buf,3,30,4);
		//		LCD_WriteSZ(102,4,dianya);
		}			  	
		else if (No==0)//返回
		{	pause=0;
			break;//按0回到主菜单
		}
		else if (No==2)//暂停
		{	pause=0;
		//	break;
		}
		else if (No==4)//修改时间
		{	//pause=0;
			modifyTime();
		//	break;
		}

		if(pause==1)
		{
	//	Delay1ms(1);
		Delay1s(1);
		miaol++;
		if(miaol==10){
		miaol=0;
		miaoh++;}
		if(miaoh==6){
		miaoh=0;
		fenl++;}
		if(fenl==10){
		fenl=0;
		fenh++;}
		if(fenh==6){
		fenh=0;
		shil++;}
		if(((shih==0)||(shih==1))&&(shil==10)){
		shil=0;
		shih++;}
		if(shih==2&&shil==4){
		shih=0;	shil=0;
		ri2++;}

		if(((ri1==0)||(ri1==1)||(!((yue1==0)&&(yue2==2))))&&(ri2==10)){
		ri2=0;
		ri1++;}
		if ((((yue1 == 0) && ((yue2 == 1) || (yue2 == 3) || (yue2 == 5) || (yue2 == 7) || (yue2 == 8))) || ((yue1 == 1) && ((yue2 == 0) || (yue2 == 2))))&&(ri1==3&&ri2==2)){//31天
		ri1=0;	ri2=1;
		yue2++;}
		if ((((yue1 == 0) && ((yue2 == 4) || (yue2 == 6) || (yue2 == 9) )) || ((yue1 == 1) && ((yue2 == 1) )))&&(ri1==3&&ri2==1)){//30天
		ri1=0;	ri2=1;
		yue2++;}
		if (((yue1==0)&&(yue2==2))&&(ri1==2&&ri2==9)){//28天
		ri1=0;	ri2=1;
		yue2++;}

		if(yue1==0&&yue2==10){
		yue2=0;yue1++;}
		if(yue1==1&&yue2==13){
		yue1=0;yue2=1;nian4++;}

		if(nian4==10){
		nian4=0;nian3++;}
		if(nian3==10){
		nian3=0;nian2++;}
		if(nian2==10){
		nian2=0;nian1++;}
		if(nian1==10){
		nian1=0;}
			


		showMiaobiao(1);

		}

	//	else
		//   break;//按0回到主菜单			  	
	//	w = GetADCValue(No);

	}

//char a1,a2,b1,b2,c1,c2,c3;


}


/*
